<div class="a-spot-hero-dark">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="card">
          <div class="carousel card-block">
            {% for item in aSpotHeroDarkTheme %}
              <div class="slide-wrapper">
                <img alt="{{ item.backgroundAlt }}" class="card-img-top"/>
                <div
                  class="jumbotron position-absolute text-white text-left jumbotron-container">
                  <h1 class="text-white">{{ item.heading }}</h1>
                  <p class="first-paragraph">{{ item.text }}</p>
                  <p>
                    <a class="text-uppercase btn btn-primary btn-sm btn-rounded ods-p-10"
                       href="#" role="button" tabindex="0">{{ item.buttonText }}</a>
                  </p>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>






